#include <stdio.h>

typedef int NodeDataType;

typedef struct tagNode
{
    NodeDataType Data;
    Node *PrevNode;
    Node *NextNode;
}Node;

Node *CreateNode(int NewData)
{
    Node NewNode = (Node *)malloc(sizeof(Node));
    NewNode->Data = NewData;
    NewNode->PrevNode = NULL;
    NewNode->NextNode = NULL;

    return NewNode;
}

void DestroyNode(Node *Node)
{
    free(Node);
}

Node *InsertNode(Node *CrtNode, Node *InsertNode)
{
    InsertNode->PrevNode = CrtNode;
    InsertNode->NextNode = CrtNode->NextNode;
    CrtNode->NextNode = InsertNode;
}

void AscendingSort(int *array, int *sorted_array, int n)
{
    Node *Node = CreateNode(array[0]);
    Node *Head = Node;

    for(i = 1; i < n; i++){
        Node *NewNode = CreateNode(array[i]);
        if(array[i] < Node->Data){
            Node = Head;
        }
        while(Node->NextNode != NULL){
            if(Node->Data < array[i]) Node = Node->NextNode;
        }
        InsertNode(Node, NewNode);
        if(Head->PrevNode != NULL) Head = Node;
    }
}

void DescendingSort()
{

}

int main()
{
    int n;
    scanf("%d",&n);

    int *array = (int *)malloc(sizeof(int) * n);
    for(i = 0; i < n; i++) *(array + i) = n - i;




    return 0;
}
